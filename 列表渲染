//项目的根组件
//APP——>index.js->public->index.html

const list=[{id:1001,name:'vue'},
            {id:1002,name:'react'},
            {id:1003,name:'angular'}]

function App(){
  return(
  <div className="App">
    this a App
    <ul>
      {list.map(item=><li key={item.id}>{item.name}</li>)}
    </ul>
  
</div>)}

export default App;
